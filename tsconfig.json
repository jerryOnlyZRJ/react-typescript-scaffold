{
    "compilerOptions": {
        "outDir": "./dist/",
        "baseUrl": "./",
        "paths": {
            "@/*": ["src/*"]
        },
        "sourceMap": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "module": "esnext",
        "target": "es5",
        "jsx": "react"
    },
    "include": [
        "./src/**/*"
    ]
}